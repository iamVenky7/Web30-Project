let raceNumber = Math.floor(Math.random() * 1000);
let early = true;

let age = 18;

if(age > 18 && early){
 raceNumber += 1000;
}

if(age > 18 && early){
  console.log(`Your race at 9:30AM and your raceNumber is: ${raceNumber}`);
} else if(early && age > 18 ){
  console.log(`Your race at 11:00AM and your raceNumber is: ${raceNumber}`);
} else if(age < 18){
  console.log('Your race at 12:30PM');
} else {
  console.log('See the Registrations desk.. Thank You!');
}
